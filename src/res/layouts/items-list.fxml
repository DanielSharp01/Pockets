<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<VBox xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ItemsListController">
    <HBox alignment="CENTER_LEFT" styleClass="horizontal-spacing">
        <padding>
            <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
        </padding>
        <TextField promptText="Search" prefWidth="300" />
        <Button minWidth="32" minHeight="32" styleClass="add-button" pickOnBounds=""/>
    </HBox>
    <ListView fx:id="itemContents" VBox.vgrow="ALWAYS">
        <padding>
            <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
        </padding>
    </ListView>
</VBox>
